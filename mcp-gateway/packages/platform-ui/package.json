{
  "name": "@forge/platform-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathIgnorePatterns='e2e|story'",
    "test:story": "jest --testPathPatterns='story\\.test\\.ts$'",
    "test:e2e": "jest --testPathPatterns='\\.e2e\\.test\\.ts$'",
    "test:smoke": "jest --testPathPatterns='\\.e2e\\.test\\.ts$' --testNamePattern='@smoke'",
    "test:sanity": "jest --testPathPatterns='\\.e2e\\.test\\.ts$' --testNamePattern='@sanity|@smoke'",
    "test:regression": "jest"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.975.0",
    "@aws-sdk/client-bedrock-runtime": "^3.975.0",
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "lucide-react": "^0.563.0",
    "next": "14.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swr": "^2.3.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "dotenv": "^17.2.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.4.0"
  }
}
