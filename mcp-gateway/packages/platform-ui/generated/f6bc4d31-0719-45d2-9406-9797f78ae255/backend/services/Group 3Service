import { PrismaClient, Group 3 } from '@prisma/client';

export class Group 3Service {
  constructor(private prisma: PrismaClient) {}

  async findAll(): Promise<Group 3[]> {
    return this.prisma.group 3.findMany();
  }

  async findById(id: string): Promise<Group 3 | null> {
    return this.prisma.group 3.findUnique({ where: { id } });
  }

  async create(data: Omit<Group 3, 'id' | 'createdAt' | 'updatedAt'>): Promise<Group 3> {
    return this.prisma.group 3.create({ data });
  }

  async update(id: string, data: Partial<Group 3>): Promise<Group 3> {
    return this.prisma.group 3.update({ where: { id }, data });
  }

  async delete(id: string): Promise<void> {
    await this.prisma.group 3.delete({ where: { id } });
  }
}

export default Group 3Service;
