import { PrismaClient, Group 12 } from '@prisma/client';

export class Group 12Service {
  constructor(private prisma: PrismaClient) {}

  async findAll(): Promise<Group 12[]> {
    return this.prisma.group 12.findMany();
  }

  async findById(id: string): Promise<Group 12 | null> {
    return this.prisma.group 12.findUnique({ where: { id } });
  }

  async create(data: Omit<Group 12, 'id' | 'createdAt' | 'updatedAt'>): Promise<Group 12> {
    return this.prisma.group 12.create({ data });
  }

  async update(id: string, data: Partial<Group 12>): Promise<Group 12> {
    return this.prisma.group 12.update({ where: { id }, data });
  }

  async delete(id: string): Promise<void> {
    await this.prisma.group 12.delete({ where: { id } });
  }
}

export default Group 12Service;
