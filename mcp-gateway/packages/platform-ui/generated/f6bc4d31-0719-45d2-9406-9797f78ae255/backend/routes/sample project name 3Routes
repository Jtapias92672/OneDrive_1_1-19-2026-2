import { Router } from 'express';
import { PrismaClient } from '@prisma/client';
import { Sample Project Name 3Controller } from '../controllers/sample project name 3.controller';
import { Sample Project Name 3Service } from '../services/sample project name 3.service';

export function createSample Project Name 3Routes(prisma: PrismaClient): Router {
  const router = Router();
  const service = new Sample Project Name 3Service(prisma);
  const controller = new Sample Project Name 3Controller(service);

  router.get('/sample project name 3s', controller.getAll.bind(controller));
  router.get('/sample project name 3s/:id', controller.getById.bind(controller));
  router.post('/sample project name 3s', controller.create.bind(controller));
  router.put('/sample project name 3s/:id', controller.update.bind(controller));
  router.delete('/sample project name 3s/:id', controller.delete.bind(controller));

  return router;
}

export default createSample Project Name 3Routes;
