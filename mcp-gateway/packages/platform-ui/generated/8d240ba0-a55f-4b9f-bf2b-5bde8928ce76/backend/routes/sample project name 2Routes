import { Router } from 'express';
import { PrismaClient } from '@prisma/client';
import { Sample Project Name 2Controller } from '../controllers/sample project name 2.controller';
import { Sample Project Name 2Service } from '../services/sample project name 2.service';

export function createSample Project Name 2Routes(prisma: PrismaClient): Router {
  const router = Router();
  const service = new Sample Project Name 2Service(prisma);
  const controller = new Sample Project Name 2Controller(service);

  router.get('/sample project name 2s', controller.getAll.bind(controller));
  router.get('/sample project name 2s/:id', controller.getById.bind(controller));
  router.post('/sample project name 2s', controller.create.bind(controller));
  router.put('/sample project name 2s/:id', controller.update.bind(controller));
  router.delete('/sample project name 2s/:id', controller.delete.bind(controller));

  return router;
}

export default createSample Project Name 2Routes;
