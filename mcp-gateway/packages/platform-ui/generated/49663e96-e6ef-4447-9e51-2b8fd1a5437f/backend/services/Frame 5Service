import { PrismaClient, Frame 5 } from '@prisma/client';

export class Frame 5Service {
  constructor(private prisma: PrismaClient) {}

  async findAll(): Promise<Frame 5[]> {
    return this.prisma.frame 5.findMany();
  }

  async findById(id: string): Promise<Frame 5 | null> {
    return this.prisma.frame 5.findUnique({ where: { id } });
  }

  async create(data: Omit<Frame 5, 'id' | 'createdAt' | 'updatedAt'>): Promise<Frame 5> {
    return this.prisma.frame 5.create({ data });
  }

  async update(id: string, data: Partial<Frame 5>): Promise<Frame 5> {
    return this.prisma.frame 5.update({ where: { id }, data });
  }

  async delete(id: string): Promise<void> {
    await this.prisma.frame 5.delete({ where: { id } });
  }
}

export default Frame 5Service;
