import { PrismaClient, Group 9 } from '@prisma/client';

export class Group 9Service {
  constructor(private prisma: PrismaClient) {}

  async findAll(): Promise<Group 9[]> {
    return this.prisma.group 9.findMany();
  }

  async findById(id: string): Promise<Group 9 | null> {
    return this.prisma.group 9.findUnique({ where: { id } });
  }

  async create(data: Omit<Group 9, 'id' | 'createdAt' | 'updatedAt'>): Promise<Group 9> {
    return this.prisma.group 9.create({ data });
  }

  async update(id: string, data: Partial<Group 9>): Promise<Group 9> {
    return this.prisma.group 9.update({ where: { id }, data });
  }

  async delete(id: string): Promise<void> {
    await this.prisma.group 9.delete({ where: { id } });
  }
}

export default Group 9Service;
